    // 对该站所有链接进行安全过滤
    $(document).ready(function () {
		$('body a').each(function () {
			var ret = $(this).attr('href');
			ret = ret && ret.replace(/<script.*?>.*?<\/script>/g, '')
                .replace(/<[^>]+>/g, '');
            $(this).attr('href', ret);
		});
	});


                adverPosition = [
                    'dbIndexPagefixedColumn',
                    'dbtvpagefixedColumn',
                    'dbmoviepagefixedColumn'
                ];
            }
            else {
                adverPosition = [
                    'dbfixedColumn1235'                    
                ];
            }
            var len = adverPosition.length;
            var idx = Math.floor(Math.random() * len);
            var adId = adverPosition[idx];
            BAIDU_VIDEO_ADM_FILL_ASYC(adId, domId);
        }();
    }();

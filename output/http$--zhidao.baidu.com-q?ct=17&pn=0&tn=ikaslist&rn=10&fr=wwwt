!function(){var F = (window.__IKNOW_GLOBAL__ || window).F;var require = (window.__IKNOW_GLOBAL__ || window).require;    var $ = require('common-new:widget/js/util/tangram/tangram');
    (function() {

        // 计算悬浮框的位置
        function countPosition () {
            var cw = document.body.clientWidth;
            if (cw < 1230) {
                $('.aria-div').css({
                    display: 'block',
                    right: 0 + 'px',
                    bottom: 60 + 'px'
                });
            }  else { 
                $('.aria-div').css({
                    display: 'block',
                    right: (cw - 1070)/2 - 80 + 'px',
                    top: '80%'
                });
            }
        }
        countPosition();

        // 当页面宽度发生变化 则重新计算位置
        $(window).on('resize', function () {
            countPosition();    
        });
    })();
}();

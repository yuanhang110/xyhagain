      /* 轮播图 */
      $('.banner-carousel').carousel({
        interval: 5000,
        touch: true
      })
      
      /* part6 swiper */
      let p6_slide_video = document.querySelectorAll('#part6_swiper .swiper-slide .bgvid')
      let part6_swiper = new Swiper('#part6_swiper', {
        pagination: {
          el: '.swiper-pagination',
          clickable :true
        },
        on: {
          slideChangeTransitionEnd: function () {
            let video_idx = this.activeIndex + 1
            let current_video = document.getElementById('bgvid' + video_idx)
            // current_video.play()
            console.log(current_video)
            p6_slide_video.forEach((item, idx) => {
              if (idx != this.activeIndex) {
                item.pause()
              }
            })
          }
        }
      })

      /* 动画初始化 */
      if (!(/msie [6|7|8|9]/i.test(navigator.userAgent))) {
        // new WOW().init();
